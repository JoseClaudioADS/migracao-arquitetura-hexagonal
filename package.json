        "express-async-errors": "^3.1.1",
        "log4js": "^6.2.1",
        "pg": "^8.0.3",
        "reflect-metadata": "^0.1.13",
        "tsyringe": "^4.1.0",
        "typeorm": "^0.2.24",
        "uuidv4": "^6.0.8",
        "yup": "^0.28.4"
    },
    "devDependencies": {
        "@babel/core": "^7.2.2",
        "@babel/preset-env": "^7.3.1",
        "@types/bcrypt": "^3.0.0",
